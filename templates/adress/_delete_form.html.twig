<form method="post" action="{{ path('adress_delete', {'id': adress.id}) }}" onsubmit="return confirm('C'est peut-être pas une bonne idée, les adresses sont utilisées par le générateur de fichiers!');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ adress.id) }}">
    <button class="btn">Delete</button>
</form>
