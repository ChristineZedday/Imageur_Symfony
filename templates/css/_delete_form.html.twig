<form method="post" action="{{ path('css_delete', {'id': css.id}) }}" onsubmit="return confirm('Voulez-vous vraiment le supprimer?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ css.id) }}">
    <button class="btn">Supprimer</button>
</form>
